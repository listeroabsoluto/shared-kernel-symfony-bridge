{
  "name": "listeroabsoluto/shared-kernel-symfony-bridge",
  "type": "library",
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2.0"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/listeroabsoluto/shared-kernel.git"
    }
  ],
  "require": {
    "php": "^8.2.0",
    "doctrine/orm": "^2.5.11",
    "listeroabsoluto/shared-kernel": "*",
    "mark-gerarts/auto-mapper-plus": "1.*",
    "symfony/dependency-injection": "^7.0",
    "symfony/doctrine-bridge": "^7.0",
    "symfony/http-foundation": "7.*",
    "symfony/messenger": "7.*",
    "symfony/serializer": "7.*",
    "symfony/uid": "7.*"
  },
  "require-dev": {
    "brianium/paratest": "^6.9",
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "SharedKernelSymfonyBridge\\": "src/SharedKernelSymfonyBridge/"
    }
  }
}
